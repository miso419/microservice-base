{
  "env": {
      "browser": false,
      "node": true,
      "mocha": true
  },
  "parserOptions": {
      "ecmaVersion": 9,
      "ecmaFeatures": {
          "impliedStrict": true
      }
  },
  "extends": "airbnb-base",
  "rules": {
    "rules": {
        "linebreak-style": [2, "windows"],
        "indent": [0, "tab"],
        "arrow-body-style": [0, "as-needed"]
    }
  }
}